<distributionCenters>
	<distributionCenter id=""> <!-- Id generated by the GUI part during creation --> 
		<initialStockLevel>INITIAL_STOCK_LEVEL_VALUE</initialStockLevel> <!-- User input -->
		<holdingCost>HOLDING_COST_VALUE</holdingCost> <!-- User input -->
		<purchaseCost>PURCHASE_COST_VALUE</purchaseCost> <!-- User input -->
		<periodLength>PERIOD_LENGTH_VALUE</periodLength> <!-- User input: Length of the simulation period (integer, in days) -->
		<demand type="array"> <!-- User input: Array of demand values for each day. Length must be equal to @periodLength value -->
			<value>DAY_0_DEMAND_VALUE</value>
			<value>DAY_1_DEMAND_VALUE</value>
			<value>DAY_2_DEMAND_VALUE</value>
			<value>DAY_3_DEMAND_VALUE</value>
			<value>DAY_4_DEMAND_VALUE</value>
			<!-- etc. -->
		</demand>
	</distributionCenter>
	
	<distributionCenter>
		<!-- etc. -->
	</distributionCenter>
	
</distributionCenters>

<externalNodes>
	<externalNode id=""></externalNode>
</externalNodes>

<edges>
	<edge id=""> <!-- Link between 2 nodes -->
		<nodeFromId></nodeFromId>
		<nodeToId></nodeToId>
		<fraction></fraction>
		<delay></delay>
	</edge>
</edges> 
